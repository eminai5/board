<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.spring.board.playerMapper">
 
    <select id="getPlayerList" parameterType="com.spring.board.dto.PlayerDto" resultType="com.spring.board.dto.PlayerDto">
    
        SELECT BOARD_SEQ 
             , BOARD_RE_REF 
             , BOARD_RE_LEV 
             , BOARD_RE_SEQ 
             , BOARD_WRITER 
             , BOARD_SUBJECT 
             , BOARD_CONTENT 
             , BOARD_HITS
             , DEL_YN 
             , INS_USER_ID
             , INS_DATE 
             , UPD_USER_ID
             , UPD_DATE 
        FROM BOARD.TB_jaehoon
    
    </select>
    
</mapper>